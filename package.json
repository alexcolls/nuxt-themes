{
  "name": "@alexcolls/nuxt-xui",
  "version": "0.3.0",
  "description": "Production-ready Nuxt 3/4 module with 50+ reusable UI components, built-in state management, dark mode, and i18n support",
  "author": {
    "name": "Alex Colls",
    "url": "https://github.com/alexcolls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcolls/nuxt-xui.git"
  },
  "bugs": {
    "url": "https://github.com/alexcolls/nuxt-xui/issues"
  },
  "homepage": "https://github.com/alexcolls/nuxt-xui#readme",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt4",
    "nuxt-module",
    "ui",
    "components",
    "vue",
    "vue3",
    "tailwind",
    "i18n",
    "dark-mode",
    "typescript",
    "layout",
    "theme",
    "state-management",
    "ssr"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.8.0",
    "@nuxt/schema": "^3.12.1 || ^4.0.0",
    "@nuxt/test-utils": "^3.19.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.51.0",
    "nuxi": "^3.29.3",
    "nuxt": "^3.12.1 || ^4.0.0",
    "typescript": "latest",
    "vitest": "^2.1.0 || ^3.0.0",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "@nuxt/ui": "^2.21.0",
    "@nuxtjs/i18n": "^8.3.1 || ^9.0.0",
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/ui": {
      "optional": false
    },
    "@nuxtjs/i18n": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
