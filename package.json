{
  "name": "@nuxt-xui/layout-ui",
  "version": "0.2.1",
  "description": "A Nuxt UI module providing reusable Layout and Common components",
  "repository": "your-username/nuxt-xui",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.1 || ^4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.8.0",
    "@nuxt/schema": "^3.12.1 || ^4.0.0",
    "@nuxt/test-utils": "^3.19.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.51.0",
    "nuxi": "^3.29.3",
    "nuxt": "^3.12.1 || ^4.0.0",
    "typescript": "latest",
    "vitest": "^2.1.0 || ^3.0.0",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "@nuxt/ui": "^2.21.0",
    "@nuxtjs/i18n": "^8.3.1 || ^9.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1 || ^0.6.0 || ^0.7.0",
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/ui": {
      "optional": false
    },
    "@nuxtjs/i18n": {
      "optional": false
    },
    "@pinia/nuxt": {
      "optional": false
    },
    "@pinia-plugin-persistedstate/nuxt": {
      "optional": false
    }
  }
}
